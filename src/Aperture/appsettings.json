{
  "Site": {
    "Name": "Aperture",
    "Copyright": {
      "Notice": "Aperture - All rights reserved",
      "StartYear": 2024
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DbSettings": {
    "Contexts": {
      "ApertureDb": {
        "Name": "ApertureDb",
        "ConnectionString": "{SqlServer-Connection-String}",
        "EnableAutomaticMigrations": true,
        "EnableDataSeeding": true
      }
    }
  },
  "Authentication": {
    "Domain": "{Auth0-Domain}",
    "ClientId": "{Auth0-ClientId}"
  },
  "AllowedHosts": "*"
}
